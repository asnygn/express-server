    "js-convert-case": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "logflake": "^2.0.5",
    "migrate-mongo": "^11.0.0",
    "mongoose": "^8.6.2",
    "mongoose-slug-plugin": "^2.1.0",
    "nodemailer": "^6.9.15",
    "qs": "^6.13.0",
