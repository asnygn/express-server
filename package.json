{
  "name": "express-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:prod": "rimraf build && tsc && tsc-alias && cross-env NODE_ENV=production node build/server.js",
    "start:dev": "cross-env NODE_ENV=development DEBUG=express-server tsx watch src/server.ts",
    "seed:prod": "cross-env NODE_ENV=production DEBUG=express-server tsx src/database/seeds/production.ts",
    "seed:dev": "cross-env NODE_ENV=development DEBUG=express-server tsx src/database/seeds/development.ts",
    "build": "rimraf build && tsc",
    "migrate-up:prod": "cross-env NODE_ENV=production migrate-mongo up",
    "migrate-down:prod": "cross-env NODE_ENV=production migrate-mongo down",
    "migrate-up:dev": "cross-env NODE_ENV=development migrate-mongo up",
    "migrate-down:dev": "cross-env NODE_ENV=development migrate-mongo down",
    "format": "npx prettier src --write",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "type": "module",
  "private": true,
  "license": "ISC",
